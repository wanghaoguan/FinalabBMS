@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <!-- 新 Bootstrap 核心 CSS 文件 -->
    <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.2/css/bootstrap.min.css">
    <!-- jQuery文件。务必在bootstrap.min.js 之前引入 -->
    <script src="~/bootstrap/jquery/jquery-1.11.3.min.js"></script>

    <!-- 最新的 Bootstrap 核心 JavaScript 文件 -->
    <script src="http://cdn.bootcss.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
    <title>成员信息 删除页面</title>
</head>
<body>
<div class="modal-content" style="width:300px">
          <div class="modal-header" style="height:40px">
            <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
            <h5 class="modal-title" style="font-family:SimHei">确认</h5>
          </div>
          <div class="modal-body" style="height:45px">
            <p>警告！请不要提交。</p>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal" style="width:30px;font-size:9px;width:50px">关闭</button>
            <button type="button" class="btn btn-primary" style="width:30px;font-size:9px;width:50px">保存</button>
          </div>
        </div>
</body>
</html>
